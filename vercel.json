{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml"
    },
    {
      "source": "/robots.txt",
      "destination": "/robots.txt"
    },
    {
      "source": "/((?!api/|_next/|static/|assets/|favicon.ico|sw.js).*)",
      "destination": "/index.html"
    }
  ]
}